import random

user_guess=int(input("Enter the number between 0-20:"))

com_guess=random.randint(0,20)

while True:
    print("computer guessed :" + str(com_guess))
    if com_guess == user_guess :
        print("computer guess right")
        break
    else:
        user=input("guess ")
        if user=='higher':
            com_guess+=1
            com_guess=random.randint(com_guess,20)
        else:
            com_guess-=1
            com_guess=random.randint(0,com_guess)
